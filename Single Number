#Solution 1

from collections import Counter
arr=[4,1,2,1,2]
arr1=dict(Counter(arr));
m=1;key=0;
for k,v in arr1.items():
    if(v<=m):                                          
        # print(k,v);
        m=v;
        key=k;
print(key)


#Solution 2

arr=[4,2,1,1,3,6,5];           
x=0;
for i in arr:
    x^=i
    # print(x);
print('ans=',x);
